{
	"name": "@iqai/adk-cli",
	"version": "0.3.11",
	"description": "CLI tool for creating, running, and testing ADK agents",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"bin": {
		"adk": "./dist/main.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsc -w -p tsconfig.json",
		"test": "echo 'No tests found in adk-cli, skipping.'",
		"test:watch": "vitest"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/IQAIcom/adk-ts.git",
		"directory": "packages/adk-cli"
	},
	"keywords": [
		"ai",
		"llm",
		"agent",
		"cli",
		"adk",
		"typescript"
	],
	"author": "IQAI",
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"@iqai/adk": "workspace:*",
		"@nestjs/common": "^11.1.6",
		"@nestjs/core": "^11.1.6",
		"@nestjs/platform-express": "^11.1.6",
		"@nestjs/swagger": "^11.2.0",
		"chalk": "^5.6.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"dedent": "^1.7.0",
		"esbuild": "^0.25.9",
		"giget": "^2.0.0",
		"marked": "^16.2.1",
		"marked-terminal": "^7.3.0",
		"nest-commander": "^3.19.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"swagger-ui-express": "^5.0.1"
	},
	"devDependencies": {
		"@iqai/tsconfig": "workspace:*",
		"@types/marked-terminal": "^6.1.1",
		"@types/node": "^24.3.1",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=22.0"
	},
	"files": [
		"dist",
		"!**/*.test.*",
		"!**/*.json",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	}
}
