{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": ["NODE_ENV"],
	"ui": "tui",
	"tasks": {
		"build": {
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [
				"dist/**",
				".next/**",
				"!.next/cache/**",
				".vercel/output/**"
			],
			"dependsOn": ["^build"]
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": []
		},
		"format": {},
		"lint": {},
		"lint:fix": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build"]
		},
		"start": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["build"]
		},
		"clean": {
			"cache": false
		}
	}
}
